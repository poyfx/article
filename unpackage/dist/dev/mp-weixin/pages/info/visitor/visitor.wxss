.visitor {
  height: 100vh;
}
.visitor .visitor_content {
    padding: 0 8.5% 0 7.5%;
}
.visitor .visitor_content .visitor_number {
      padding: 20px 0 21px;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
      -webkit-align-content: center;
              align-content: center;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
}
.visitor .visitor_content .visitor_number text {
        color: #888888;
        font-size: 0.65rem;
}
.visitor .visitor_content .visitor_number view {
        -webkit-align-content: center;
                align-content: center;
        -webkit-box-align: center;
        -webkit-align-items: center;
                align-items: center;
}
.visitor .visitor_content .visitor_number view image {
          width: 6px;
          height: 11px;
          margin-left: 5px;
}
.visitor .visitor_content .visitor_number view text {
          color: #B38B6F;
}
.visitor .visitor_content .visitor_many .visitor_list {
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
      -webkit-align-content: center;
              align-content: center;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
}
.visitor .visitor_content .visitor_many .visitor_list .list_content {
        padding: 12px 0 13px;
        -webkit-align-content: center;
                align-content: center;
        -webkit-box-align: center;
        -webkit-align-items: center;
                align-items: center;
}
.visitor .visitor_content .visitor_many .visitor_list .list_content .list_img {
          margin-right: 13px;
}
.visitor .visitor_content .visitor_many .visitor_list .list_content .list_img image {
            width: 44px;
            height: 44px;
}
.visitor .visitor_content .visitor_many .visitor_list .list_content .list_name view {
          font-size: 0.75rem;
          color: #555555;
          font-weight: 550;
}
.visitor .visitor_content .visitor_many .visitor_list .list_content .list_name text {
          color: #888888;
          font-size: 0.65rem;
}
.visitor .visitor_content .visitor_many .visitor_list .list_right image {
        width: 6px;
        height: 11px;
}

